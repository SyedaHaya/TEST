rows = int(input("Enter number of rows: "))

for i in range(1, rows + 1):
    
    if i == 3:
        for j in range(2):
            print("*", end=" ")
        print()
        
    elif i > 5:
        break
        
    else:
        for j in range(i):
            print("*", end=" ")
        print()
